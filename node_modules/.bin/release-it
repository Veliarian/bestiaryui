#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/veliar/Projects/bestiaryui/node_modules/.pnpm/release-it@19.2.2_@types+node@25.0.3/node_modules/release-it/bin/node_modules:/Users/veliar/Projects/bestiaryui/node_modules/.pnpm/release-it@19.2.2_@types+node@25.0.3/node_modules/release-it/node_modules:/Users/veliar/Projects/bestiaryui/node_modules/.pnpm/release-it@19.2.2_@types+node@25.0.3/node_modules:/Users/veliar/Projects/bestiaryui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/veliar/Projects/bestiaryui/node_modules/.pnpm/release-it@19.2.2_@types+node@25.0.3/node_modules/release-it/bin/node_modules:/Users/veliar/Projects/bestiaryui/node_modules/.pnpm/release-it@19.2.2_@types+node@25.0.3/node_modules/release-it/node_modules:/Users/veliar/Projects/bestiaryui/node_modules/.pnpm/release-it@19.2.2_@types+node@25.0.3/node_modules:/Users/veliar/Projects/bestiaryui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../release-it/bin/release-it.js" "$@"
else
  exec node  "$basedir/../release-it/bin/release-it.js" "$@"
fi
